{
  "env": {
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "$": "readonly",
    "$$": "readonly",
    "browser": "readonly"
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["jasmine"],
  "extends": ["eslint:recommended", "plugin:jasmine/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "jasmine/no-focused-tests": 0
  }
}
